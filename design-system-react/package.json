{
  "name": "@leouo/design-system-react",
  "sideEffects": false,
  "version": "0.0.2",
  "description": "React wrapper for my personal design system core",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "npm run tsc",
    "tsc": "tsc -p .",
    "rollup": "rollup -c"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@types/jest": "23.3.9",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "jest": "^23.0.0",
    "jest-dom": "^3.0.2",
    "np": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^3.3.4000"
  },
  "dependencies": {
    "@leouo/design-system-core": "^0.0.1"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leouo/my-design-system.git"
